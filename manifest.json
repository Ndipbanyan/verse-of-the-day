{
	"manifest_version": 3,
	"name": "Verse of the Day",
	"version": "2.2",
	"description": "Bring your new tab page to life with YouVersion's verse of the day.",
	"action": {},

	"icons": {
		"16": "16.png",
		"32": "32.png",
		"192": "192.png"
	},

	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"host_permissions": ["*://*/*"],
	"permissions": ["storage", "unlimitedStorage"],
	"background": {
		"service_worker": "background.js"
	},
	"minimum_chrome_version": "88"
}
